<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

	<!--
	    This overrides the default true value in the same resource folder in preference library.
	    The app doesn't have icons for preferences, so it's better to hide the empty space.
	-->
	<!-- lint:MissingDefaultResource the default resource is in the library's values folder. -->
	<!-- lint:UnusedResources REPORT false positive, it is clearly used indicated by override=true. -->
	<bool name="config_materialPreferenceIconSpaceReserved"
		tools:ignore="MissingDefaultResource,UnusedResources"
		tools:override="true">false</bool>

</resources>
