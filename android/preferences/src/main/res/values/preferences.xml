<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

	<!-- lint:UnusedResources Some of these are defined here to have every preference in one place,
	     but they're actually only used in different modules. -->

	<string name="pref_version" translatable="false">version</string>
	<integer name="pref_version_default" translatable="false">2</integer>

	<string name="pref_currentLanguage" translatable="false" tools:ignore="UnusedResources">currentLanguage</string>

	<string name="pref_group_preferences">Preferences</string>
	<string name="pref_group_links">Links</string>
	<string name="pref_group_advanced">Advanced</string>
	<string name="pref_group_internal" translatable="false">Internal</string>

	<string name="pref_defaultViewPage_title">Default Details Page</string>
	<string name="pref_defaultViewPage" translatable="false">defaultViewPage</string>
	<string name="pref_defaultViewPage_default" translatable="false">@string/pref_defaultViewPage_auto</string>
	<!-- Certain places, for example opening a search result, may benefit from displaying a different page in the header. -->
	<string name="pref_defaultViewPage_auto_title">Automatic</string>
	<string name="pref_defaultViewPage_auto" translatable="false">auto</string>
	<string name="pref_defaultViewPage_image_title">Image</string>
	<string name="pref_defaultViewPage_image" translatable="false">image</string>
	<string name="pref_defaultViewPage_details_title">Details &amp; Statistics</string>
	<string name="pref_defaultViewPage_details" translatable="false">details</string>
	<string-array name="pref_defaultViewPage_entries" translatable="false">
		<item>@string/pref_defaultViewPage_auto_title</item>
		<item>@string/pref_defaultViewPage_image_title</item>
		<item>@string/pref_defaultViewPage_details_title</item>
	</string-array>
	<string-array name="pref_defaultViewPage_values" translatable="false">
		<item>@string/pref_defaultViewPage_auto</item>
		<item>@string/pref_defaultViewPage_image</item>
		<item>@string/pref_defaultViewPage_details</item>
	</string-array>

	<string name="pref_suggestCategory_title">Suggest Categories</string>
	<string name="pref_suggestCategory" translatable="false">suggestCategory</string>
	<string name="pref_suggestCategory_default" translatable="false">@string/pref_suggestCategory_unmatched</string>
	<!-- Suggest Categories every time regardless of current selection. -->
	<string name="pref_suggestCategory_always_title">Always</string>
	<string name="pref_suggestCategory_always" translatable="false">always</string>
	<!-- Suggest Categories when suggestions contain better ones than current. -->
	<string name="pref_suggestCategory_unmatched_title">When better</string>
	<string name="pref_suggestCategory_unmatched" translatable="false">unmatched</string>
	<!-- Never suggest Categories. Manual suggestion is still there via action_category_suggest in context menu -->
	<string name="pref_suggestCategory_manual_title">Never</string>
	<string name="pref_suggestCategory_manual" translatable="false">manual</string>
	<string-array name="pref_suggestCategory_entries" translatable="false">
		<item>@string/pref_suggestCategory_always_title</item>
		<item>@string/pref_suggestCategory_unmatched_title</item>
		<item>@string/pref_suggestCategory_manual_title</item>
	</string-array>
	<string-array name="pref_suggestCategory_values" translatable="false">
		<item>@string/pref_suggestCategory_always</item>
		<item>@string/pref_suggestCategory_unmatched</item>
		<item>@string/pref_suggestCategory_manual</item>
	</string-array>

	<string name="pref_highlightSuggestion_title">Highlight Matches</string>
	<string name="pref_highlightSuggestion" translatable="false">highlightSuggestions</string>
	<bool name="pref_highlightSuggestion_default" translatable="false">false</bool>
	<string name="pref_highlightSuggestion_desc">Display the edit distance of the entered name and the match
		in the suggestions list.</string>

	<string name="pref_internalImageViewer_title">Use internal image viewer</string>
	<string name="pref_internalImageViewer" translatable="false">internalImageViewer</string>
	<bool name="pref_internalImageViewer_default" translatable="false">true</bool>
	<string name="pref_internalImageViewer_desc">Internal image viewer has limited functionality,
		but it definitely shows the images,
		while using another application may result in crashes out of this app\'s control.</string>

	<string name="pref_displayDebugDetails_title">Display more details (debug)</string>
	<string name="pref_displayDebugDetails" translatable="false">displayDebugDetails</string>
	<bool name="pref_displayDebugDetails_default" translatable="false">false</bool>
	<string name="pref_displayDebugDetails_desc">Display some internal details of the entities,
		these information are only relevant for debugging and hacking.</string>

	<string name="pref_sunburstIgnoreLevel_title">Sunburst Ignore Level</string>
	<string name="pref_sunburstIgnoreLevel" translatable="false">sunburstIgnoreLevel</string>
	<integer name="pref_sunburstIgnoreLevel_default" translatable="false">5</integer>
	<string name="pref_sunburstIgnoreLevel_desc">Sunburst diagram can be slow if there are a lot of items shown.
		Decrease this setting to ignore items stored deep within other items from displaying. Values below 3 are not advised.</string>

	<string name="pref_showWelcome" translatable="false">showWelcome</string>
	<bool name="pref_showWelcome_default" translatable="false">true</bool>

	<string name="pref_app_in_settings_title">App info in Settings</string>
	<string name="pref_app_in_settings_desc">manage storage, permissions, notifications, etc.</string>

	<string name="pref_app_in_store_title">App details in Play Store</string>
	<string name="pref_app_in_store_desc">check for updates, leave a review, share, uninstall, etc.</string>

	<string name="pref_app_privacy_policy_title">Privacy Policy</string>
	<string name="pref_app_privacy_policy_desc">How is my data handled?</string>

	<string name="pref_app_feedback_title">Feedback</string>
	<string name="pref_app_feedback_desc">Ask a question, request features you\'d like to see, or just tell me what you think about the app. Any feedback is welcome.</string>
	<string name="pref_app_feedback_body">Write your request/question/feedback here.</string>
	<string name="pref_app_feedback_subject">Magic Home Inventory feedback</string>

	<!-- Will be replaced with "About" during resource merging. -->
	<string name="pref_app_about_title">@string/merged_resource</string>
	<!-- Will be replaced with "Magic Home Inventory" during resource merging. -->
	<string name="pref_app_about_desc">@string/merged_resource</string>
	<!-- Will be replaced with "net.twisterrob.inventory" during resource merging. -->
	<string name="pref_app_about_package">@string/merged_resource</string>

	<!-- Will be replaced with "Manage Inventory Space" during resource merging. -->
	<string name="pref_app_manage_space_title">@string/merged_resource</string>
</resources>
