<?xml version="1.0" encoding="utf-8"?>
<!-- lint:MergeRootFrame this is inflated as a ViewPager, need to extra level -->
<FrameLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	tools:ignore="MergeRootFrame"
	>
	<ImageView
		android:id="@id/image"
		android:layout_width="fill_parent"
		android:layout_height="fill_parent"
		android:contentDescription="@string/image_title"
		android:scaleType="fitCenter"
		tools:src="@drawable/category_unknown"
		/>
	<ImageView
		android:id="@id/type"
		android:layout_width="@dimen/icon_action_button"
		android:layout_height="@dimen/icon_action_button"
		android:layout_margin="@dimen/unit_half"
		android:layout_gravity="bottom|end"
		android:contentDescription="@string/item_categorize"
		tools:src="@drawable/category_unknown"
		/>
</FrameLayout>
